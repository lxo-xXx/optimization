* Calculate critical temperature difference for fluid selection
PARAMETER delta_T_critical(fluids);
delta_T_critical(fluids) = 443.15 - fluid_props(fluids, 'Tc');

* Display fluids suitable for the application (delta_T between 35-50Â°C)
PARAMETER suitable_fluids(fluids);
suitable_fluids(fluids)$(delta_T_critical(fluids) >= 35 AND delta_T_critical(fluids) <= 50) = 1;

DISPLAY suitable_fluids, delta_T_critical;

* Calculate ideal gas enthalpy function
SCALAR T_ref /298.15/;
PARAMETER cp_avg(fluids);
cp_avg(fluids) = cp_coeffs(fluids,'a') + 
                 cp_coeffs(fluids,'b') * 400 + 
                 cp_coeffs(fluids,'c') * 400**2 + 
                 cp_coeffs(fluids,'d') * 400**3 + 
                 cp_coeffs(fluids,'e') * 400**4 + 
                 cp_coeffs(fluids,'f') * 400**5;

DISPLAY cp_avg;